{
  "name": "openmined-website",
  "homepage": "https://www.openmined.org",
  "version": "1.0.5",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "forcedomain": "^0.8.1",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^4.1.3",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "openmined-ui": "^0.0.6",
    "prettier": "^1.11.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-frontload": "^1.0.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^1.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "build-css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css":
      "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "serve":
      "NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 node ./server/index.js",
    "deploy": "NODE_ENV=production node ./server/index.js",
    "eject": "react-scripts eject"
  }
}
